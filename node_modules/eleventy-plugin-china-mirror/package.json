{
  "name": "eleventy-plugin-china-mirror",
  "version": "1.0.3",
  "description": "Zero-config China network acceleration plugin for Eleventy - speeds up sharp, libvips downloads and cdn.11ty.dev requests",
  "keywords": [
    "eleventy-plugin",
    "china",
    "mirror",
    "accelerate",
    "npm",
    "registry",
    "sharp",
    "libvips",
    "cdn"
  ],
  "main": "index.js",
