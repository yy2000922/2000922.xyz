  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    }
  },
  "bin": {
    "create-eleventy-china": "./cli.js"
  },
  "engines": {
    "node": ">=16"
  },
  "peerDependencies": {
    "@11ty/eleventy": ">=2.0.0"
  },
  "files": [
    "index.js",
    "index.cjs",
    "cli.js",
    "README.md",
    "LICENSE",
    "example"
  ],
